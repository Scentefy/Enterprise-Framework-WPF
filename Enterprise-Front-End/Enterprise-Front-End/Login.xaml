<Page x:Class="Enterprise_Front_End.Page3"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Enterprise_Front_End"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="Page3">

    <Grid>
        <TextBox x:Name="usernamebox" HorizontalAlignment="Left" Height="23" Margin="90,126,0,0" TextWrapping="Wrap" Text="Username" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Left" Margin="90,154,0,0" VerticalAlignment="Top" Width="120" ToolTip="Password" Height="23"/>
        <Label x:Name="label" Content="Sign In" HorizontalAlignment="Left" Margin="127,71,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button" Content="Sign In" HorizontalAlignment="Left" Margin="90,196,0,0" VerticalAlignment="Top" Width="120" Click="button_Click"/>

    </Grid>
</Page>
